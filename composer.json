{
    "name": "roberhuang/designpatterns",
    "description": "Design Patterns",
	"keywords": ["design", "patterns"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Huang",
            "email": "roberhuang@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
	"require-dev": {
        "phpunit/phpunit": "^7.5"
    },
	"autoload": {
        "psr-4": {
            "Roberhuang\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Roberhuang\\DesignPatterns\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
